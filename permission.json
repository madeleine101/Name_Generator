{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Allow Gebruiker to upload objects",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::381491851433:user/Gebruiker"
            },
            "Action": [
            "s3:PutObject"
            ],
            "Resource": "arn:aws:s3:::namen/*"
        }
    ]
}
